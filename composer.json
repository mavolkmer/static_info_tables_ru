{
  "repositories": {
	  "static-info-tables": {
		  "type": "vcs",
		  "url": "https://codeberg.org/mvoehringer/static-info-tables"
	  }
  },
  "keywords": [
    "TYPO3 CMS",
    "Static Info Tables"
  ],
  "name": "mselbach/static-info-tables-ru",
  "description": "(ru) language pack for the Static Info Tables providing localized names for countries, currencies and so on.",
  "type": "typo3-cms-extension",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Manuel Selbach",
      "role": "Developer"
    }
  ],
  "replace": {
    "mselbach/static-info-tables-ru": "self.version"
  },
  "require": {
    "php": ">=7.2",
    "typo3/cms-core": "^10.4 || ^11.5",
    "sjbr/static-info-tables": "dev-TYPO3-11"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.8",
    "nimut/testing-framework": "2.x-dev || 3.x-dev || 4.x-dev || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "Mselbach\\StaticInfoTablesRu\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mselbach\\StaticInfoTablesRu\\Tests\\": "Tests"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "static_info_tables_ru",
      "app-dir": ".Build",
      "web-dir": ".Build/public"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/public/typo3conf/ext/",
      "[ -L .Build/public/typo3conf/ext/static_info_tables_ru ] || ln -snvf ../../../../. .Build/public/typo3conf/ext/static_info_tables_ru"
    ],
    "extension-cleanup-for-release": [
      "rm -rf Tests/",
      "rm -f .gitignore",
      "rm -f .travis.yml",
      "rm -f composer.lock"
    ]
  }
}
